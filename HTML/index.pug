<!DOCTYPE html>
html
  head
    title Kalkulator
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js" type="text/javascript")
    script(src="../JS/main.js" type="text/javascript")
    LINK(rel='stylesheet' href="../CSS/main.min.css")
  body
    .calculator
      .numbers
        - var i=0
          while (i++<9)
            .circle
              li=i
          .circle
            li=0
          .circle
            li="."
          .circle.cancel
            li="C"
      .math-options
        .fundamental
          .circle.plus +
          .circle.minus -
          .circle.x *
          .circle.divide /
        .extra
          .equation =
          .circle.pi Ï€
          .circle.left-racket (
          .circle.right-bracket )
          .circle.e e


script(src="http://localhost:35729/livereload.js")
